html,
body,
div,
header,
nav,
li,
ul,
ol,
footer,
section,
aside,
input,
article,
h1,
h2,
h3,
h4,
h5 {
  margin: 0;
  padding: 0;
  /* 盒子模型的 怪异模式  */
  box-sizing: border-box;
}
html,
body {
  font-size: 100px;
}
a {
  text-decoration: none;
  color: inherit;
  display: block;
  /* 移动端 a标签 点击高亮 */
}
ul {
  list-style: none;
}
img {
  width: 100%;
  /* 解决图片默认的3像素  */
  display: block;
}
input {
  display: block;
  width: 100%;
  height: 100%;
  border: none;
}
/* 清楚浮动 */
.clearfix::after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
/*字体图标*/
@font-face {
  font-family: 'iconfont';
  src: url('../font/iconfont.eot');
  src: url('../font/iconfont.eot?#iefix') format('embedded-opentype'), url('../font/iconfont.woff') format('woff'), url('../font/iconfont.ttf') format('truetype'), url('../font/iconfont.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
header {
  width: 100%;
  height: 0.44rem;
  background-color: #ff841d;
  position: fixed;
  z-index: 1;
  top: 0;
}
header a {
  display: block;
  position: relative;
}
header a .iconfont {
  left: 0.10rem;
  top: .12rem;
  width: 0.1rem;
  height: .16rem;
  position: absolute;
  color: white;
  font-weight: bold;
  font-size: 0.17rem;
}
header .discount {
  width: 100%;
  height: 0.44rem;
  font-size: 0.20rem;
  text-align: center;
  line-height: 0.4rem;
  color: white;
  font-weight: bold;
}
header .AppLoad {
  top: 0.05rem;
  right: 0.20rem;
  background: url(../images/header_app.png) center / 0.3rem no-repeat;
  width: .34rem;
  height: .34rem;
  position: absolute;
}
.taoUl {
  margin-top: 0.44rem ;
}
.content-Img {
  padding: .14rem .08rem;
  height: 1.28rem;
  width: 100%;
}
.content-Img a {
  height: 1.0rem;
  position: relative;
}
.content-Img a .pic {
  left: 0;
  position: absolute;
}
.content-Img a .pic img {
  width: 1.0rem;
  height: 1.0rem;
}
.content-Img a .info {
  height: 1rem;
  width: 100%;
  padding-left: 1.2rem;
  padding-top: 0.05rem;
}
.content-Img a .info .title {
  font-size: 0.14rem;
  width: 100%;
  height: 0.6rem;
}
.content-Img a .info .title span {
  color: red;
  font-size: .15rem;
}
.content-Img a .info .title-tool {
  float: left;
  width: 100%;
  height: 0.5rem;
  font-size: .12rem;
}
.content-Img a .info .title-tool span {
  margin-top: 0.1rem;
  float: left;
}
.content-Img a .info .title-tool .brand {
  color: #a1a1a1;
}
.content-Img a .info .title-tool .iconfont {
  font-size: .12rem;
  float: right;
  color: #a1a1a1;
}
.footer-page {
  font-size: .14rem;
  height: .77rem;
  width: 100%;
  padding: .2rem .3rem;
  background-color: #f2f2f2;
  position: relative;
}
.footer-page table {
  text-align: center;
}
.footer-page table .mid-selectPage {
  width: 100%;
  text-align: center;
}
.footer-page table .mid-selectPage #selectPage {
  width: 1.3rem;
  height: .32rem;
  text-align: center;
}
.footer-page table .mid-selectPage #selectPage option {
  text-align: center;
}
.footer-page table a {
  display: block;
  line-height: .3rem;
}
.footer-page table .prePage {
  position: absolute;
  height: .3rem;
  width: .82rem;
  border: 1px solid #bababa;
  left: .4rem;
  border-radius: .05rem;
  background-color: #dfdfdf;
}
.footer-page table .nextPage {
  position: absolute;
  height: .3rem;
  width: .82rem;
  border: 1px solid #bababa;
  right: .4rem;
  border-radius: .05rem;
  background-color: #dfdfdf;
}
.footer-tools {
  font-size: .14rem;
  height: .4rem;
  border: 1px solid #cccccc;
  width: 100%;
  background-color: #f3f3f3;
  padding-left: .1rem;
}
.footer-tools .content li {
  float: left;
  line-height: .4rem;
  margin-right: .05rem;
}
