html,
body,
div,
header,
nav,
li,
ul,
ol,
footer,
section,
aside,
input,
article,
h1,
h2,
h3,
h4,
h5 {
  margin: 0;
  padding: 0;
  /* 盒子模型的 怪异模式  */
  box-sizing: border-box;
}
a {
  text-decoration: none!important;
  color: inherit;
  display: block;
  /* 移动端 a标签 点击高亮 */
  -webkit-tap-highlight-color: transparent;
}
a:focus {
  text-decoration: none!important;
}
a:hover {
  text-decoration: none!important;
}
ul {
  list-style: none;
}
img {
  width: 100%;
  /* 解决图片默认的3像素  */
  display: block;
}
input {
  display: block;
  width: 100%;
  height: 100%;
  border: none;
}
/* 清楚浮动 */
.clearfix::after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
@font-face {
  font-family: 'iconfont';
  src: url('../font2/iconfont.eot');
  src: url('../font2/iconfont.eot?#iefix') format('embedded-opentype'), url('../fon2/iconfont.woff') format('woff'), url('../font2/iconfont.ttf') format('truetype'), url('../font2/iconfont.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont";
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.view {
  width: 100%;
  clear: both;
  background-color: #f2f2f2;
  /*头部*/
}
.view header {
  width: 100%;
  height: 44px;
  background-color: #ff841d;
  position: fixed;
  top: 0;
  z-index: 999;
}
.view header .back {
  width: 0.5rem;
  height: 0.44rem;
  position: absolute;
  left: 0.1rem;
}
.view header .back a {
  width: 100%;
  height: 44px;
  line-height: 44px;
  color: #fff;
  background-position: 50px 0px;
  font-size: 0.17rem;
}
.view header .back a i {
  font-style: none;
  font-size: 17px;
}
.view header h1 {
  font-size: 0.2rem;
  text-align: center;
  font-weight: bold;
  line-height: 44px;
  color: #fff;
  margin: 0px 0.52rem;
}
.view header .home {
  width: 50px;
  position: absolute;
  height: 44px;
  right: 10px;
  top: 0;
}
.view header .home a {
  position: relative;
  width: 100%;
  height: 44px;
  background: url("../images/header_app.png") center / 32px no-repeat;
}
.view .cu-friend {
  width: 100%;
  background-color: #fff;
  text-align: left;
  font-size: 0.14rem;
  margin: 0 auto;
}
.view .cu-friend ul {
  width: 100%;
  padding: 0px 0px 0.1rem 0px;
}
.view .cu-friend ul li {
  float: left;
  padding: 0.1rem 0 0 0.1rem;
  text-align: center;
  width: 50%;
  border-right: 1px solid #eee;
  border-bottom: 1px solid #eee;
  background-color: #fff;
}
.view .cu-friend ul li a {
  display: block;
  width: 100%;
  height: 100%;
}
.view .cu-friend ul li a .img {
  margin: 0.1rem auto;
  width: 1.4rem;
  height: 1.4rem;
  overflow: hidden;
}
.view .cu-friend ul li a .img img {
  width: 1.4rem;
  height: 1.4rem;
}
.view .cu-friend ul li a .title {
  color: #333;
  padding: 0 0.08rem;
  font-size: 0.14rem;
  text-align: left;
  line-height: 0.18rem;
  height: 0.36rem;
  overflow: hidden;
}
.view .cu-friend ul li a .subtit {
  color: #ff6c00;
  margin-top: 0.04rem;
  font-size: 0.14rem;
  padding: 0 0.08rem;
  text-align: left;
  line-height: 0.18rem;
  overflow: hidden;
  height: 0.18rem;
}
.view .cu-friend ul li a .info {
  padding: 0.06rem 0.08rem;
  font-size: 0.14rem;
  overflow: hidden;
  line-height: 0.24rem;
}
.view .cu-friend ul li a .info .mall {
  text-align: left;
  color: #a1a1a1;
  font-size: 0.12rem;
}
.view .Page_loading {
  margin-top: 0.15rem;
  text-align: center;
  font-size: 0.16rem;
  background-color: #f8f8f8;
  height: 0.32rem;
  clear: both;
}
.view .Page_loading em {
  margin: 0 auto;
}
.view .Page_loading em img {
  width: 0.32rem;
  height: 0.32rem;
  margin: 0 auto;
}
.view .ct100_Menu {
  margin-top: 0.2rem;
  background: linear-gradient(to bottom, #ffffff, #ebebeb);
  border-top: 1px solid #ebebeb;
  border-bottom: 1px solid #ccc;
  font-size: 0.14rem;
  padding-left: 0.08rem;
  color: #000;
  line-height: 0.36rem;
}
.view .ct100_Menu a {
  float: left;
  color: #000;
}
.view #ctl00_footerApp .foot_top {
  background: #efefef;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  height: 0.34rem;
  line-height: 0.34rem;
  margin-top: 0.25rem;
  clear: both;
}
.view #ctl00_footerApp .foot_top a {
  float: left;
  border-right: 1px solid #c4c4c4;
  text-align: center;
  width: 30%;
  font-size: 0.14rem;
  color: #000;
}
.view #ctl00_footerApp .foot_top a img {
  display: inline-block;
  width: 0.15rem;
  margin-top: -0.05rem;
}
.view #ctl00_footerApp .footer {
  background: #efefef;
  text-align: center;
  font-size: 0.12rem;
  padding: 0.15rem 0;
  color: #666;
}
.view #ctl00_footerApp .footer p {
  padding: 0;
  margin: 0;
}
.view #ctl00_footerApp .footer a {
  display: inline-block;
  color: #000;
}
.slowbuy {
  position: fixed;
  right: 0;
  bottom: 0.06rem;
  z-index: 999;
}
.slowbuy a {
  width: 0.6rem;
  display: block;
}
