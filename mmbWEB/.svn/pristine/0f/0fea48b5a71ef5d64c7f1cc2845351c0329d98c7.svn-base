html,
body,
div,
header,
nav,
li,
ul,
ol,
footer,
section,
aside,
input,
article,
h1,
h2,
h3,
h4,
h5 {
  margin: 0;
  padding: 0;
  /* 盒子模型的 怪异模式  */
  box-sizing: border-box;
}
a {
  text-decoration: none!important;
  color: inherit;
  display: block;
  /* 移动端 a标签 点击高亮 */
  -webkit-tap-highlight-color: transparent;
}
a:focus {
  text-decoration: none!important;
}
a:hover {
  text-decoration: none!important;
}
ul {
  list-style: none;
}
img {
  width: 100%;
  /* 解决图片默认的3像素  */
  display: block;
}
input {
  display: block;
  width: 100%;
  height: 100%;
  border: none;
}
/* 清楚浮动 */
.clearfix::after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
