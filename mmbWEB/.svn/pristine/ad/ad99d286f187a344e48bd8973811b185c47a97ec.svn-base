html,
body,
div,
header,
nav,
li,
ul,
ol,
footer,
section,
aside,
input,
article,
h1,
h2,
h3,
h4,
h5 {
  margin: 0;
  padding: 0;
  /* 盒子模型的 怪异模式  */
  box-sizing: border-box;
}
a {
  text-decoration: none!important;
  color: inherit;
  display: block;
  /* 移动端 a标签 点击高亮 */
  -webkit-tap-highlight-color: transparent;
}
a:focus {
  text-decoration: none!important;
}
a:hover {
  text-decoration: none!important;
}
ul {
  list-style: none;
}
img {
  width: 100%;
  /* 解决图片默认的3像素  */
  display: block;
}
input {
  display: block;
  width: 100%;
  height: 100%;
  border: none;
}
/* 清楚浮动 */
.clearfix::after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
@font-face {
  font-family: 'iconfont';
  src: url('../font/iconfont.eot');
  src: url('../font/iconfont.eot?#iefix') format('embedded-opentype'), url('../fon/iconfont.woff') format('woff'), url('../font/iconfont.ttf') format('truetype'), url('../font/iconfont.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont";
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
body {
  font-size: 0.16rem;
}
.lowestPrice_container .lowestPrice_header {
  background-color: #E4393C;
  width: 100%;
  height: 45px;
  position: fixed;
  top: 0;
  padding: 0 45px;
  z-index: 999;
}
.lowestPrice_container .lowestPrice_header .LP_back {
  width: 45px;
  height: 45px;
  position: absolute;
  top: 0;
  left: 0;
  text-align: center;
  line-height: 45px;
}
.lowestPrice_container .lowestPrice_header .LP_back a {
  font-size: 30px;
  color: #fff;
}
.lowestPrice_container .lowestPrice_header .LP_title {
  width: 100%;
  height: 45px;
}
.lowestPrice_container .lowestPrice_header .LP_title .title_content {
  text-align: center;
  font-size: 20px;
  color: #fff;
}
.lowestPrice_container .lowestPrice_header .LP_title .title_content p {
  margin: 0;
  line-height: 45px;
  font-weight: 700;
}
.lowestPrice_container .lowestPrice_header .LP_updata {
  width: 45px;
  height: 45px;
  background: url("../images/header_app.png") no-repeat center;
  background-size: 35px 35px;
  position: absolute;
  top: 0;
  right: 0;
}
.lowestPrice_container .LP_navBar {
  background-color: #fff;
  margin-top: 45px;
  position: relative;
  overflow: hidden;
}
.lowestPrice_container .LP_navBar .LP_nav {
  height: 35px;
  padding-right: 35px;
  overflow: hidden;
}
.lowestPrice_container .LP_navBar .LP_nav ul {
  width: 750px;
  position: absolute;
  left: 0;
}
.lowestPrice_container .LP_navBar .LP_nav ul li {
  float: left;
  text-align: center;
  height: 34px;
  line-height: 35px;
  padding: 0 10px;
  font-size: 14px;
}
.lowestPrice_container .LP_navBar .LP_nav ul li a {
  height: 34px;
}
.lowestPrice_container .LP_navBar .LP_nav .LP_nav_search {
  width: 35px;
  height: 35px;
  background-color: #f7f7f7;
  border-left: 1px solid #f7f7f7;
  position: absolute;
  right: 0;
  top: 0;
}
.lowestPrice_container .LP_navBar .LP_nav .LP_nav_search a {
  display: block;
  width: 100%;
  text-align: center;
  line-height: 35px;
  font-size: 30px;
}
.lowestPrice_container .LP_navBar .LP_nav .LP_nav_search2 {
  width: 35px;
  height: 35px;
  background-color: #f7f7f7;
  border-left: 1px solid #f7f7f7;
  position: absolute;
  right: 0;
  top: 0;
}
.lowestPrice_container .LP_navBar .LP_nav .LP_nav_search2 a {
  display: block;
  width: 100%;
  text-align: center;
  line-height: 35px;
  font-size: 20px;
  font-weight: 400;
  color: gray;
}
.lowestPrice_container .LP_navBar .LP_nav .LP_searchBar {
  width: 100%;
  padding: 20px 60px 20px 20px ;
  background-color: #f7f7f7;
  position: absolute;
  top: 35px;
  left: 0;
  z-index: 20;
  display: none;
}
.lowestPrice_container .LP_navBar .LP_nav .LP_searchBar input {
  width: 100%;
  height: 30px;
}
.lowestPrice_container .LP_navBar .LP_nav .LP_searchBar button {
  position: absolute;
  top: 50%;
  right: 10px;
  transform: translateY(-50%);
  border: none;
  background-color: #f7f7f7;
  font-size: 16px;
}
.lowestPrice_container .LP_navBar .LP_img {
  padding: 15px 0;
  background-color: #f7f7f7;
}
.lowestPrice_container .rocket {
  width: 40px;
  height: 40px;
  border-radius: 50%;
  background-color: gray;
  text-align: center;
  line-height: 40px;
  position: fixed;
  bottom: 0.5rem;
  right: 0.5rem;
  z-index: 100;
  display: none;
}
.lowestPrice_container .rocket i {
  font-size: 24px;
}
.lowestPrice_container .LP_goodsList {
  background-color: #f7f7f7;
}
.lowestPrice_container .LP_goodsList .goods {
  min-height: 150px;
  background-color: #fff;
  margin-bottom: 15px;
  padding: 10px 5px;
  padding-left: 1.4rem;
  position: relative;
  overflow: hidden;
}
.lowestPrice_container .LP_goodsList .goods .goods_img {
  width: 1.3rem;
  position: absolute;
  top: .1rem;
  left: .05rem;
  margin-right: .2rem;
}
.lowestPrice_container .LP_goodsList .goods .goods_img img {
  height: 1.3rem;
}
.lowestPrice_container .LP_goodsList .goods .goods_info {
  width: 100%;
  padding: 0 .2rem;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_title {
  min-height: 28px;
  max-height: 40px;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_price {
  font-size: 0.2rem;
  color: #fc4e52;
  margin-bottom: 10px;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_price em {
  font-size: 0.2rem;
  color: #fc4e52;
  padding: 0 .1rem;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_price del {
  color: gray;
  font-size: 0.16rem;
  margin-left: 10px;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_progress {
  min-height: 26px;
  height: .26rem;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_progress .bar {
  float: left;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_progress .bar i span {
  display: inline-block;
  width: 1rem;
  height: .15rem;
  line-height: .15rem;
  min-height: 12px;
  border-radius: 10px;
  background-color: #ffb7b7;
  border: 1px solid red;
  text-align: center;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_progress .bar b {
  color: #fc4e52;
  margin-left: 10px;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_links {
  width: 100%;
  height: .3rem;
  max-height: 31px;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_links a:nth-child(1) {
  width: 1.5rem;
  text-align: center;
  float: left;
  border: 1px dotted #fc4e52;
  color: #fc4e52;
  padding: .05rem;
  font-size: .16rem;
}
.lowestPrice_container .LP_goodsList .goods .goods_info .info_links a:nth-child(2) {
  width: .8rem;
  float: right;
  background-color: #E4393C;
  padding: 5px;
  color: #fff;
  border-radius: 5px;
  font-size: .16rem;
  text-align: center;
}
.lowestPrice_container .LP_bottom {
  width: 100%;
  position: fixed;
  bottom: 0;
  font-size: 14px;
  color: #5a5a5a;
  left: 0;
  padding: 20px 0;
  background-color: #f7f7f7;
  max-height: 150px;
}
.lowestPrice_container .LP_bottom ul li {
  float: left;
  width: 33.3%;
  height: 30px;
  line-height: 30px;
  box-sizing: border-box;
  border: 1px solid #efefef;
  text-align: center;
}
.lowestPrice_container .LP_bottom .bottom_info p:nth-child(1) {
  padding: .2rem;
  margin: 0;
  text-align: center;
}
.lowestPrice_container .LP_bottom .bottom_info p:nth-child(1) span:nth-child(1) {
  color: red;
  padding: 0 .1rem;
}
.lowestPrice_container .LP_bottom .bottom_info p:nth-child(1) span:nth-child(2) {
  font-weight: 700;
}
.lowestPrice_container .LP_bottom .bottom_info p:nth-child(1) span:nth-child(3) {
  color: #747474;
}
.lowestPrice_container .LP_bottom .bottom_info p:nth-child(2) {
  margin: 0;
  text-align: center;
  color: #bfbfbf;
}
.active {
  border-bottom: 1px solid #E4393C;
  color: red;
}
