.w {
  margin: 0 0.15rem;
}
.coupon_top {
  width: 100%;
  background-color: #f4483e;
}
.coupon_top .coupon_title {
  padding: 0 0.5rem;
  position: relative;
}
.coupon_top .coupon_title .coupon_back {
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
}
.coupon_top .coupon_title .coupon_back a {
  font-size: 0.2rem;
}
.coupon_top .coupon_title .coupon_back a i {
  font-weight: 700;
  font-size: 0.4rem;
  color: #fff;
}
.coupon_top .coupon_title h2 {
  margin: 0;
  font-size: 30px;
  text-align: center;
  line-height: 0.6rem;
  color: #fff;
  font-weight: 400;
}
.coupon_top .coupon_title .coupon_down {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
}
.coupon_top .coupon_title .coupon_down i {
  font-size: 0.4rem;
  display: block;
  height: 0.4rem;
  width: 0.5rem;
  background: url("../images/header_app.png") no-repeat center / contain;
}
