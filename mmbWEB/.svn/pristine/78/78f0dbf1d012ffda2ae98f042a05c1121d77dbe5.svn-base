@import "media.css";
* {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
@font-face {
  font-family: 'iconfont';
  src: url('../font/font_cyzuv1n9sn12a9k9/iconfont.eot');
  src: url('../font/font_cyzuv1n9sn12a9k9/iconfont.eot?#iefix') format('embedded-opentype'), url('../font/font_cyzuv1n9sn12a9k9/iconfont.woff') format('woff'), url('../font/font_cyzuv1n9sn12a9k9/iconfont.ttf') format('truetype'), url('../font/font_cyzuv1n9sn12a9k9/iconfont.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
a {
  text-decoration: none;
  display: block;
  -webkit-tap-highlight-color: transparent;
  color: inherit;
}
img {
  width: 100%;
  display: block;
}
.clearfix:after {
  display: block;
  content: "";
  height: 0;
  clear: both;
}
#backToPrevious {
  font-size: 30px;
}
.w {
  margin: 0 10px;
}
html,
body {
  height: 100%;
}
.coupon_bg {
  width: 100%;
  height: 100%;
  z-index: 100;
  position: fixed;
  background-color: rgba(51, 51, 51, 0.9);
}
.coupon_bg .coupon_bg_light {
  width: 160px;
  height: 128px;
  position: fixed;
  overflow: hidden;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  z-index: 100;
}
.coupon_bg .coupon_bg_light .coupon_bg_carousel .coupon_image {
  float: left;
  width: 160px;
}
.coupon_bg span {
  position: fixed;
  display: block;
  top: 50%;
  text-align: center;
  text-decoration: none;
  font-size: 28px;
  line-height: 58px;
  color: #767676;
  width: 43px;
  height: 58px;
  border-radius: 5px;
  z-index: 100;
  cursor: pointer;
  background-color: rgba(36, 36, 36, 0.9);
}
.coupon_bg span:nth-last-of-type(1) {
  left: 5%;
  transform: translateY(-50%);
}
.coupon_bg span:nth-last-of-type(2) {
  right: 5%;
  transform: translateY(-50%);
}
.view {
  height: 100%;
  position: relative;
}
.view .coupon_top {
  height: 44px;
  width: 100%;
  background-color: #f4483e;
}
.view .coupon_top .coupon_title {
  padding: 0 50px;
  position: relative;
}
.view .coupon_top .coupon_title .coupon_back {
  position: absolute;
  left: 0;
  height: 44px;
  width: 50px;
}
.view .coupon_top .coupon_title .coupon_back a {
  display: block;
  width: 100%;
  height: 100%;
  text-decoration: none;
}
.view .coupon_top .coupon_title .coupon_back a i {
  font-weight: 700;
  display: block;
  font-size: 17px;
  color: #fff;
  line-height: 44px;
}
.view .coupon_top .coupon_title h2 {
  margin: 0 52px;
  font-size: 20px;
  text-align: center;
  line-height: 44px;
  color: #fff;
  font-weight: 400;
}
.view .coupon_top .coupon_title .coupon_down {
  position: absolute;
  top: 0;
  right: 0;
  height: 44px;
  width: 50px;
}
.view .coupon_top .coupon_title .coupon_down i {
  display: block;
  height: 44px;
  width: 36px;
  background: url("../images/header_app.png") no-repeat center / contain;
}
.view .coupon_merchant {
  margin: 0 5px;
}
.view .coupon_merchant .coupon_area ul {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.view .coupon_merchant .coupon_area ul li {
  margin-top: 5px;
  border: 1px solid #efefef;
  display: block;
  width: 33.3%;
  height: 82px;
}
.view .coupon_merchant .coupon_area ul li a {
  display: block;
  padding-top: 5px;
  width: 100%;
  height: 75px;
  font-size: 12px;
  text-align: center;
  color: #5a5a5a;
}
.view .coupon_merchant .coupon_area ul li a img {
  display: block;
  width: 50px;
  height: 50px;
  margin: 0 auto;
}
.view .couponList_lis .couponList_lis_bar {
  color: #999;
  font-size: 12px;
  text-align: center;
  margin: 8px 0;
}
.view .couponList_lis .coupon_table {
  width: 100%;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}
.view .couponList_lis .coupon_table a {
  display: block;
  padding: 10px;
  font-size: 12px;
  text-decoration: none;
  position: relative;
}
.view .couponList_lis .coupon_table a .coupon_info {
  padding-left: 100px;
}
.view .couponList_lis .coupon_table a .coupon_info .coupon_img {
  position: absolute;
  left: 10px;
}
.view .couponList_lis .coupon_table a .coupon_info .coupon_img img {
  width: 100px;
  height: 80px;
}
.view .couponList_lis .coupon_table a .coupon_info h4 {
  font-size: 14px;
  color: #333;
  height: 40px;
  line-height: 20px;
  padding-left: 10px;
  margin: 0;
  overflow: hidden;
}
.view .couponList_lis .coupon_table a .coupon_info .coupon_price {
  padding-left: 10px;
}
.view .couponList_lis .coupon_table a .coupon_info .coupon_price h3 {
  margin: 0;
  font-size: 14px;
  color: #5a5a5a;
  height: 24px;
  line-height: 24px;
  font-weight: 700;
}
.view .couponList_lis .coupon_table a .coupon_info .coupon_time {
  padding: 10px 0 0 10px;
  color: #EE7700;
  font-size: 14px;
  line-height: 24px;
}
.view .coupon_tool {
  width: 100%;
  height: 36px;
  margin-top: 20px;
  border-top: 1px solid #fff;
  border-bottom: 1px solid #ccc;
  background-image: -moz-linear-gradient(top, #ffffff, #efefef);
  background-image: -webkit-linear-gradient(top, #ffffff, #efefef);
}
.view .coupon_tool .coupon_tool_box {
  margin-left: 8px;
  font-size: 12px;
}
.view .coupon_tool .coupon_tool_box a {
  display: inline-block;
  text-decoration: none;
  line-height: 36px;
  color: #000;
}
.view .coupon_footer {
  width: 100%;
  padding-top: 25px;
  height: 98px;
}
.view .coupon_footer .coupon_login {
  background-color: #efefef;
  border-top: 1px solid #ccc;
  display: flex;
}
.view .coupon_footer .coupon_login a {
  display: block;
  font-size: 14px;
  height: 36px;
  text-align: center;
  text-decoration: none;
  line-height: 36px;
}
.view .coupon_footer .coupon_login a:nth-child(1) {
  flex: 2;
  border-right: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}
.view .coupon_footer .coupon_login a:nth-child(2) {
  flex: 2;
  border-right: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}
.view .coupon_footer .coupon_login a:nth-child(3) {
  flex: 3;
  border-bottom: 1px solid #ccc;
}
.view .coupon_footer .coupon_login a:nth-child(3) img {
  display: inline-block;
  width: 15px;
  height: 15px;
  margin: 0 auto;
  margin: 5px 2px 0 0;
  vertical-align: baseline;
}
.view .coupon_footer .coupon_footerbottom {
  background-color: #efefef;
  height: 62px;
  font-size: 14px;
}
.view .coupon_footer .coupon_footerbottom .coupon_footinfo {
  padding-top: 15px;
  text-align: center;
}
.view .coupon_footer .coupon_footerbottom .coupon_footinfo a {
  display: inline-block;
}
.view .coupon_footer .coupon_footerbottom .coupon_footinfo a:nth-child(1) {
  color: red;
}
.view .coupon_footer .coupon_footerbottom .coupon_footurl {
  text-align: center;
}
.view .coupon-poster {
  position: fixed;
  right: 5px;
  bottom: 30px;
}
.view .coupon-poster img {
  width: 64px;
  height: 71px;
}
