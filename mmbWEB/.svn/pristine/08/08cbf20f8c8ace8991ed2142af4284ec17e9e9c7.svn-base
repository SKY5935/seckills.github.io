                <div class="other">
                    <span class="source">{{$value.productFrom}} | {{$value.productTime}}</span>
                    <span class="iconfont">&#xe622{{$value.productComCount}}</span>
                </div>
            </div>
        </a>
    </li>
    {{/each}}
</script>